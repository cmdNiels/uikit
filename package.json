{
	"name": "@cmdniels/uikit",
	"version": "1.4.6",
	"private": false,
	"license": "MIT",
	"type": "module",
	"main": "./out/index.js",
	"types": "./out/index.d.ts",
	"exports": {
		".": {
			"types": "./out/index.d.ts",
			"import": "./out/index.js"
		},
		"./cn": {
			"types": "./out/cn.d.ts",
			"import": "./out/cn.js"
		},
		"./clerk": {
			"types": "./out/clerk.d.ts",
			"import": "./out/clerk.js"
		},
		"./tailwind.css": "./tailwind.css"
	},
	"files": [
		"out",
		"tailwind.css",
		"README.md",
		"LICENSE"
	],
	"sideEffects": false,
	"scripts": {
		"format": "bunx prettier --write .",
		"lint": "bunx eslint .",
		"lint:fix": "bunx eslint --fix .",
		"fix": "bun run lint:fix && bun run format",
		"dev": "bunx ladle serve",
		"build:docs": "bunx ladle build",
		"preview": "bunx ladle preview",
		"build": "bun run build:clean && bun run build:tsc && bun run build:alias",
		"build:clean": "bunx rimraf out",
		"build:tsc": "bunx tsc --project tsconfig.build.json",
		"build:alias": "bunx tsc-alias -p tsconfig.build.json",
		"prepublishOnly": "bun run build"
	},
	"peerDependencies": {
		"@clerk/themes": "^2.4.46",
		"next": "^14.0.0 || ^15.0.0 || ^16.0.0",
		"react": "^18.0.0 || ^19.0.0",
		"react-dom": "^18.0.0 || ^19.0.0",
		"recharts": "^3.6.0"
	},
	"dependencies": {
		"@base-ui/react": "^1.0.0",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^9.0.0",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@tabler/icons-react": "^3.35.0",
		"@tailwindcss/postcss": "^4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"motion": "^12.23.26",
		"next-themes": "^0.4.6",
		"react-day-picker": "^9.12.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4",
		"tw-animate-css": "^1.4.0"
	},
	"devDependencies": {
		"@clerk/themes": "^2.4.46",
		"@cmdniels/eslint-config": "^1.0.1",
		"@ladle/react": "^5.1.1",
		"@types/node": "24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"eslint": "9.39.1",
		"next": "^16.0.10",
		"prettier": "~3.7.4",
		"react": "19.2.1",
		"react-dom": "19.2.1",
		"shadcn": "^3.6.1",
		"tsc-alias": "^1.8.16",
		"typescript": "^5",
		"vite": "^5.4.21"
	},
	"packageManager": "bun@1.3.0",
	"engines": {
		"bun": ">=1.3.0"
	}
}
